{
  "name": "thaidatepicker-react",
  "version": "0.2.1",
  "description": "Thaidatepicker-react is a component for ReactJS that likes other DatePicker, but all we need is Buddhist Year (25XX â€“ aka Thai Year) come with the right render day (example: Sat, 29 Feb 2020 must be equal to Sat, 29 Feb 2563) so I wish this component will be a useful thing to you :D. Happy Coding.",
  "author": "buildingwatsize",
  "license": "MIT",
  "repository": "buildingwatsize/thaidatepicker-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "lint:fix": "eslint . --fix",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "clean": "rm -rf ./dist/* ./example/build/*",
    "predeploy": "yarn run clean && yarn run prepublish && cd example && yarn link \"thaidatepicker-react\" && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "cross-env": "^7.0.2",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "gh-pages": "^3.2.3",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "^5.0.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@testing-library/react": "^13.1.1",
    "antd": "^4.20.1",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "react-datepicker": "^4.7.0",
    "react-test-renderer": "^18.1.0"
  },
  "keywords": [
    "react",
    "datepicker",
    "buddhist",
    "thai"
  ]
}